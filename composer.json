{
    "name": "mgkprod/laravel-mgk-auth",
    "description": "Log users using MGK SSO server",
    "keywords": [
        "mgkprod",
        "mgk-auth"
    ],
    "homepage": "https://github.com/mgkprod/mgk-auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Simon Rubuano",
            "email": "sr@mgk.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MGK\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MGK\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MGK\\Auth\\AuthServiceProvider"
            ]
        }
    }
}